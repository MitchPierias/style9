{
  "name": "style9",
  "version": "0.5.9",
  "description": "CSS-in-JS compiler",
  "author": "Johan Holmerin <johan@holmer.in>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/johanholmerin/style9.git"
  },
  "keywords": [
    "styles",
    "css",
    "css-in-js",
    "babel",
    "babel-plugin",
    "gatsby",
    "gatsby-plugin",
    "next",
    "nextjs",
    "nextjs-plugin",
    "webpack",
    "rollup",
    "rollup-plugin"
  ],
  "dependencies": {
    "@babel/core": "^7.10.4",
    "@babel/types": "^7.8.3",
    "@rollup/pluginutils": "^3.0.4",
    "known-css-properties": "^0.19.0",
    "loader-utils": "1.4.0",
    "murmurhash-js": "^1.0.0",
    "postcss": "^7.0.32",
    "postcss-discard-duplicates": "^4.0.2",
    "postcss-selector-parser": "^6.0.2",
    "sort-css-media-queries": "^1.5.0",
    "webpack-virtual-modules": "^0.4.1"
  },
  "devDependencies": {
    "@babel/plugin-transform-modules-commonjs": "^7.10.4",
    "@types/jest": "^26.0.20",
    "@types/murmurhash-js": "^1.0.3",
    "@typescript-eslint/eslint-plugin": "^4.15.1",
    "@typescript-eslint/parser": "^4.15.1",
    "eslint": "^7.20.0",
    "eslint-config-prettier": "^8.0.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.1.5",
    "eslint-plugin-jest-formatting": "^2.0.1",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "jest": "^26.1.1",
    "prettier": "^2.2.1",
    "prettier-eslint": "^12.0.0",
    "rollup": "^1.29.0",
    "ts-jest": "^26.5.1",
    "typescript": "^4.1.5",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11"
  },
  "scripts": {
    "__test__": "",
    "test": "yarn test:all",
    "test:all": "jest --coverage",
    "test:watch": "jest --coverage=false --watch",
    "__lint__": "",
    "lint": "yarn lint:all",
    "lint:all": "yarn lint:code:all && yarn lint:code:all",
    "lint:fix": "yarn lint:code:fix && yarn lint:code:fix",
    "lint:code:all": "eslint -c ./.eslintrc '**/*.{js,ts,tsx}'",
    "lint:code:fix": "eslint -c ./.eslintrc '**/*.{js,ts,tsx}' --fix",
    "lint:styles:all": "yarn stylelint './packages/**/*.less'",
    "lint:styles:fix": "yarn stylelint './packages/**/*.less' --fix"
  }
}
